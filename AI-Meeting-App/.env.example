# OpenAI API Key for transcription and analysis
OPENAI_API_KEY=your_openai_api_key_here

# Google Calendar Integration
# Option 1: Service Account Key File Path
GOOGLE_SERVICE_ACCOUNT_KEY_FILE=path/to/service-account-key.json

# Option 2: Service Account Credentials JSON (as string)
GOOGLE_SERVICE_ACCOUNT_CREDENTIALS={"type":"service_account","project_id":"your-project-id",...}

# Database (if using PostgreSQL in production)
DATABASE_URL=postgresql://username:password@localhost:5432/meetingai

# Development
NODE_ENV=development
PORT=5000
